@{
    Layout = "~/Views/Shared/Control.cshtml";
}
@model IList<RecNote.Entities.Audio.Audio>
<section id="audioList">
    <ul>
        @foreach(var audio in Model)
        {
            <li>
                <section class="audio" audioID="@audio.ID">
                    <a class="play" onclick="audio.play('@audio.ID')" ></a>
                    <h3>@audio.Name</h3>
                    @audio.Date
                </section>
            </li>
        }
    </ul>
</section>