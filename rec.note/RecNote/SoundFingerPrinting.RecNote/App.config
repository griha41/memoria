<?xml version="1.0"?>
<configuration>
    
  <configSections>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core"/>
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core"/>
    </sectionGroup>
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging"/>
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <spring>
    <context>
      <!-- using embedded assembly configuration file -->
      <resource uri="config://spring/objects"/>
      
      <resource uri="~/SoundFingerPrint.xml"/>
      <resource uri="~/Data.xml"/>
      <resource uri="~/Domain.xml"/>
    </context>
    <objects xmlns="http://www.springframework.net">
      <!-- Incorpora variables de configuraciÃ³n a contexto Spring -->
      <object type="Spring.Objects.Factory.Config.PropertyPlaceholderConfigurer, Spring.Core">
        <property name="configSections" value="appSettings"/>
      </object>
    </objects>
  </spring>
</configuration>
